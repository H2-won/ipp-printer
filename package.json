{
  "name": "ipp-printer-tutorial",
  "version": "1.0.0",
  "description": "ipp-printer-tutorial with Electron/TS",
  "main": "./dist/main.js",
  "scripts": {
    "tsc": "tsc",
    "start": "yarn run tsc && electron ./dist/main.js",
    "build:win": "electron-builder --windows nsis:ia32"
  },
  "license": "MIT",
  "dependencies": {
    "eslint": "^7.29.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "ipp-printer": "^1.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "axios": "^0.21.1",
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^2.3.5",
    "form-data": "^4.0.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "socket.io": "^4.1.2"
  },
  "build": {
    "productName": "ipp-printer-tutorial",
    "appId": "Electron-Tutorial APP",
    "asar": true,
    "protocols": {
      "name": "Electron-Tutorial",
      "schemes": [
        "Electron-Tutorial"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./huiicon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./dist/",
      "app": "."
    }
  }
}
